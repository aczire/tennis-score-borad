  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${fileDirname}",
      "env": {
        "RDSDBHOST": "localhost",
        "RDSDBPORT": "3306",
        "RDSDBNAME": "cbdbdatastorelocal",
        "RDSDBUSERNAME": "cblocaladmin",
        "RDSDBPASSWORD": "123456",
        "DB_CONFIG": "RDS"
      }
    }
  ]
}
